@model IEnumerable<WebApplication1.Models.Customer>

@{
    ViewBag.Title = "Customers";
}

<h2>Customer</h2>


<table class="table">
    <tr>

        <th>
            @("Customers")
            
        </th>
        
    </tr>

@foreach (var Customer in Model) {
    <tr>
        <td>@Html.ActionLink(Customer.Name + " " + Customer.Surname, "Details", "Customers", new { id = Customer.Id },null)</td>
    </tr>
}

</table>
