@model IEnumerable<WebApplication1.Models.Movie>

@{
    ViewBag.Title = "ListMovies";
}

<h2>Movies</h2>


<table class="table">
    <tr>
        <th>
            @("Movie")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FilmName)
        </td>

    </tr>
}

</table>
